{
  "name": "all-in-one",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll 500 --host 0.0.0.0 --port 4200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --poll 500",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:frontend": "docker-compose up frontend-aio",
    "docker:test": "docker-compose up frontend-test-aio",
    "build:upload": "npm run build && firebase deploy",
    "firebase:emulator:start": "firebase emulators:start --import ./src/app/core/mocks/firestore",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.3",
    "@angular/cdk": "^13.2.3",
    "@angular/common": "~13.2.3",
    "@angular/compiler": "~13.2.3",
    "@angular/core": "~13.2.3",
    "@angular/fire": "^7.2.0",
    "@angular/forms": "~13.2.3",
    "@angular/platform-browser": "~13.2.3",
    "@angular/platform-browser-dynamic": "~13.2.3",
    "@angular/router": "~13.2.3",
    "@angular/service-worker": "~13.2.3",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "firebase": "^9.6.3",
    "primeflex": "^3.1.0",
    "primeicons": "^5.0.0",
    "primeng": "^13.2.0",
    "rxfire": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.4",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.2.4",
    "@angular/compiler-cli": "~13.2.3",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "eslint-plugin-ngrx": "^2.0.0",
    "typescript": "~4.5.5"
  }
}
